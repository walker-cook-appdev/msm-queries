<h1>List of all actors</h1>

<table border="1">
<tr> 
<td>ID</td>
<td>Image</td>
<td>Name</td>
<td>DOB</td>
<td></td>
</tr>
<% Actor.all.each do |a_thing|%>
<tr>
  <td><%=a_thing.id%></td>
  <td><img src="http://robohash.org/<%=a_thing.image%>"></td>
  <td><%=a_thing.name%></td>
  <td><%=a_thing.dob%></td>
  <td> 
  <a href="/actors/<%=a_thing.id%>"> 
  Show details
  </a> 
  </td>
</tr>
<%end%>
</table>